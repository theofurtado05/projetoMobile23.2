<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaCadastro"
    android:background="#0098a6">

    <ImageView
        android:id="@+id/logo3"
        android:layout_width="149dp"
        android:layout_height="145dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.11"
        app:srcCompat="@drawable/log" />

    <EditText
        android:id="@+id/email3"
        android:layout_width="309dp"
        android:layout_height="57dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/redondo"
        android:ems="13"
        android:fontFamily="@font/inter_semibold"
        android:hint="E-mail"
        android:inputType="textEmailAddress"
        android:padding="9dp"
        app:layout_constraintBottom_toTopOf="@+id/senha2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo3"
        app:layout_constraintVertical_bias="0.105" />

    <EditText
        android:id="@+id/senha2"
        android:layout_width="309dp"
        android:layout_height="57dp"
        android:background="@drawable/redondo"
        android:ems="13"
        android:fontFamily="@font/inter_semibold"
        android:hint="Senha"
        android:inputType="textPassword"
        android:padding="9dp"
        app:layout_constraintBottom_toTopOf="@+id/confirmeSenha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo3"
        app:layout_constraintVertical_bias="0.831" />

    <EditText
        android:id="@+id/confirmeSenha"
        android:layout_width="309dp"
        android:layout_height="57dp"
        android:background="@drawable/redondo"
        android:ems="13"
        android:fontFamily="@font/inter_semibold"
        android:hint="Conforme sua senha"
        android:inputType="textPassword"
        android:padding="9dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo3"
        app:layout_constraintVertical_bias="0.466" />

    <TextView
        android:id="@+id/mostrar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:text="Mostrar"
        android:textColor="#61BFD4"
        app:layout_constraintBottom_toBottomOf="@+id/senha2"
        app:layout_constraintEnd_toEndOf="@+id/senha2"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="@+id/senha2"
        app:layout_constraintTop_toTopOf="@+id/senha2"
        app:layout_constraintVertical_bias="0.594" />

    <TextView
        android:id="@+id/mostrar3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:text="Mostrar"
        android:textColor="#61BFD4"
        app:layout_constraintBottom_toBottomOf="@+id/confirmeSenha"
        app:layout_constraintEnd_toEndOf="@+id/confirmeSenha"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="@+id/confirmeSenha"
        app:layout_constraintTop_toTopOf="@+id/confirmeSenha"
        app:layout_constraintVertical_bias="0.594" />

    <Button
        android:id="@+id/registrar"
        android:layout_width="297dp"
        android:layout_height="57dp"
        android:backgroundTint="#61BFD4"
        android:background="@drawable/redondo"
        android:fontFamily="@font/inter_semibold"
        android:text="Registrar"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/tenhoConta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmeSenha" />

    <TextView
        android:id="@+id/tenhoConta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:text="JÃ¡ tenho conta"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirmeSenha" />

</androidx.constraintlayout.widget.ConstraintLayout>